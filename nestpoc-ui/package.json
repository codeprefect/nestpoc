{
  "name": "nest-ui",
  "version": "0.0.0",
  "scripts": {
    "prestart": "ngtw build",
    "ng": "ng",
    "start": "ng serve && ngtw watch",
    "build": "ngtw build && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "yarn cypress:run",
    "cypress:run": "concurrently \"ng serve --prod\" \"cypress run\"",
    "cypress:open": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.0",
    "@types/supertest": "^2.0.7",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "cypress": "^3.3.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "reflect-metadata": "^0.1.12",
    "supertest": "^3.4.1",
    "tailwindcss": "^1.0.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": ">=3.1.1 <3.3",
    "xmlhttprequest": "^1.8.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "moduleNameMapper": {
      "@nestpoc/(.*)": "<rootDir>/src/modules/$1"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!<rootDir>/**/*.spec.ts",
      "!<rootDir>/**/index.ts",
      "!<rootDir>/src/*.ts",
      "!<rootDir>/**/*.d.ts",
      "!<rootDir>/**/dist/**/*.d.ts",
      "!**/__mocks__/**/*.ts"
    ]
  }
}
